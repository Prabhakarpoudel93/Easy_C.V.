<!DOCTYPE html>
<html>

<head>
    <link rel="stylesheet" href="style.css">
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/6.4.2/css/all.min.css"
        integrity="sha512-z3gLpd7yknf1YoNbCzqRKc4qyor8gaKU1qmn+CShxbuBusANI9QpRohGBreCFkKxLhei6S9CQXFEbbKuqLg0DA=="
        crossorigin="anonymous" referrerpolicy="no-referrer" />

</head>

<body>

    <div class="main-container" id="mainId">

        <div class="cv-container">
            <h1>CV Builder</h1>
            <div class="button">
                <button class="previous-btn btn-common">Previous</button>
                <button class="next-btn btn-common" id="nBtn">Next-></button>

            </div>

            <!-- Professional Details Container--div0-->
            <div class="container div-common div-0 " id="personalDetailContainer">

                <form class="form ">
                    <div class="image-div ">

                        <p>
                        <h2 class="cvmain-title ">Personal Details</h2>
                        </p>
                        <h5 class="cvmain-title">Profile Picture</h2>
                            <div class="image-box">
                                <img type="file" src="" alt="" id="image" class="image">
                                <input type="file" id="imageInput">
                            </div>


                    </div>

                    <div class="sub-container">
                        <div class="sub-div">
                            <label for="first_name">First Name:</label>
                            <input type="text" id="first_name" name="first_name" class="common">
                        </div>

                        <div class="sub-div">
                            <label for="last_name">Last Name:</label>
                            <input type="text" id="last_name" name="last_name" class="common">
                        </div>
                    </div>

                    <div class="sub-container ">
                        <div class="sub-div">
                            <label for="phone">Phone:</label>
                            <input type="text" id="phone" name="phone" class="form-common">
                        </div>

                        <div class="sub-div">
                            <label for="email_address">Email Address:</label>
                            <input type="text" id="email_address" name="email_address" class="form-common">
                        </div>

                    </div>

                    <div class="address">
                        <label for="address">Address:</label>
                        <textarea name="address" id="address" class="textarea"></textarea>
                    </div>


                    <div class="sub-container ">
                        <div class="sub-div">
                            <label for="city">City:</label>
                            <input type="text" id="city" name="city" class="form-common">
                        </div>

                        <div class="sub-div">
                            <label for="post_code">Post Code:</label>
                            <input type="text" id="post_code" name="post_code" class="form-common">
                        </div>
                    </div>

                    <div class="sub-container ">
                        <div class="sub-div">
                            <label for="state">State:</label>
                            <input type="text" id="state" name="state" class="form-common">
                        </div>

                        <div class="sub-div">
                            <label for="country">Country:</label>
                            <input type="text" id="country" name="country" class="form-common">
                        </div>

                    </div>

                </form>
            </div>


            <!-- Professional Experience Container-div1 -->

            <div class="container div-common div-1" id="professionalExperienceContainer">

                <p>
                <h2 class=" cv-title ">Professional Experience</h2>
                <h5 class=" cv-title ">Tell us about your most recent job</h5>
                </p>
                <form class="form1">
                    <div class="sub-container">
                        <div class="sub-div">
                            <label for="position_title_1">Position Title:</label>
                            <input type="text" id="position_title_1" name="position_title" class="titleInput common"
                                required>
                        </div>

                        <div class="sub-div">
                            <label for="company_name_1">Company Name:</label>
                            <input type="text" id="company_name_1" name="company_name" class="common" required>
                        </div>
                    </div>

                    <div class="sub-container ">
                        <div class="sub-div">
                            <label for="start_date_1">Start Date:</label>
                            <input type="date" id="start_date_1" name="start_date" class="form-common" required>
                        </div>

                        <div class="sub-div">
                            <label for="end_date_1">End Date:</label>
                            <input type="date" id="end_date_1" name="end_date" class="form-common" required>
                        </div>
                    </div>

                    <div class="address">
                        <p class="summary">Work Summary</p>
                        <textarea name="work" id="work_1" class="textarea"></textarea>
                    </div>
                </form>

                <div id="addWorkButton" class="addmore">
                    <button class="btn-common" onclick="addWorkExperience()">Add More</button>

                </div>
            </div>

            <!-- Education --div2 -->
            <div id="education" class=" container div-common div-2">
                <p>
                <h2 class="cv-title">Education</h2>
                <h5 class=" cv-title ">Add your most relevant education</h5>
                </p>
                <form class="form ">
                    <div class="sub-container">
                        <div class="sub-div">
                            <label for="school_name1">School Name:</label>
                            <input type="text" id="school_name1" name="school_name" class="common titleInput" required>
                        </div>

                        <div class="sub-div">
                            <label for="school_location1">School Location:</label>
                            <input type="text" id="school_location1" name="school_location" class="common" required>
                        </div>

                    </div>

                    <div class="sub-container ">

                        <div class="sub-div">
                            <label for="start_date1">Start Date:</label>
                            <input type="date" id="start_date1" name="start_date" class="form-common" required>
                        </div>

                        <div class="sub-div">
                            <label for="end_date1">End Date:</label>
                            <input type="date" id="end_date1" name="end_date" class="form-common" required>

                        </div>

                    </div>

                    <div class="sub-container">
                        <div class="sub-div">
                            <label for="degree1">Degree:</label>
                            <input type="text" id="degree1" name="degree" class="common" required>
                        </div>

                        <div class="sub-div">
                            <label for="field_study1">Field of Study:</label>
                            <input type="text" id="field_study1" name="field_study" class="common" required>
                        </div>

                    </div>

                    <div class="address">
                        <p class="summary">Description</p>
                        <textarea name="work" id="work1" class="textarea"></textarea>
                    </div>
                </form>

                <div class="addmore" id="eduButton">
                    <button class="btn-common" onclick="addEduction()">Add more</button>
                </div>
            </div>


            <!-- Skill Section -->
            <div id="skill" class=" container div-common div-3">


                <div class="main-form">
                    <p>
                    <h2 class="cv-title">Skill</h2>
                    <h5 class=" cv-title ">Add relevant professional key skills</h5>
                    </p>
                    <form class="form  ">
                        <div class="skill" id="skillDiv">
                            <div class="skill-div">
                                <label for="skillName1">Skill:</label>
                                <input type="text" id="skillName1" name="skillName" class="common" required>
                                <input type="range" min="1" max="5" value="2.5" class="slider " id="myRange1">(1-5)
                            </div>
                            <div class="addmore" id="skill-btn">
                                <button class="btn-common" onclick="addSkill()">Add more</button>
                            </div>
                        </div>
                    </form>

                </div>

            </div>

            <!-- Achievement -->
            <section id="others" class="container div-common div-4">

                <h3 class="cv-title">Others</h3>
                <form>
                    <div class="others">
                        <div class="otherTitle">
                            <input type="text" id="othersTitle1" name="achievement" placeholder="Title">
                        </div>
                        <div class="address">
                            <p class="summary">Description</p>
                            <textarea name="work" id="othersDescription1" class="textarea"></textarea>
                        </div>
                        <div class="addmore" id="others-btn">
                            <button class="btn-common" onclick="addOthers()">Add more</button>
                        </div>
                    </div>
                </form>

            </section>


            <!-- References -->

            <div class="container div-common div-5 " id="References">

                <form class="form ">
                    <div class="img-div ">

                        <p>
                        <h2 class="cv-title">References</h2>
                        </p>
                    </div>

                    <div class="sub-container">
                        <div class="sub-div">
                            <label for="fname1">First Name:</label>
                            <input type="text" id="fname1" name="fname" class="common">
                        </div>

                        <div class="sub-div">
                            <label for="lname1">Last Name:</label>
                            <input type="text" id="lname1" name="lname1" class="common">
                        </div>
                    </div>

                    <div class="sub-container ">
                        <div class="sub-div">
                            <label for="phone1">Phone:</label>
                            <input type="text" id="phone1" name="phone1" class="form-common">
                        </div>

                        <div class="sub-div">
                            <label for="eaddress1">Email Address:</label>
                            <input type="text" id="eaddress1" name="eaddress1" class="form-common">
                        </div>

                    </div>

                    <div class="sub-container">
                        <div class="sub-div">
                            <label for="ptitle1">Position Title:</label>
                            <input type="text" id="ptitle1" name="ptitle1" class="titleInput common" required>
                        </div>

                        <div class="sub-div">
                            <label for="cname1">Company Name:</label>
                            <input type="text" id="cname1" name="cname1" class="common" required>
                        </div>
                    </div>



                    <div class="address">
                        <p class="summary">Work Relation</p>
                        <textarea name="work" id="work1" class="textarea"></textarea>
                    </div>



                </form>
                <div class="addmore" id="references-btn">
                    <button class="btn-common" onclick="addReferences()">Add more</button>
                </div>
            </div>


            <!-- Submit Button -->


            <div class="finish common-template">
                <button id="finishCV" class="finish-btn common-template btn-common"
                    onclick="generateCV()">Finish</button>
            </div>

        </div>

        <!-- Template Div -->
        <div class="template-div" id="template_div">

            <div class="div4 commom-template" id="personalDetailsTemplate">

            </div>

            <div class="div4 common-template" id="workExperienceTemplate">
                <h2>Work Experience</h2>

            </div>
            <div class="div4 common-template" id="educationTemplate">
                <h2>Education</h2>

            </div>


            <div class="skill common-template" id="skillDivTemplate">
                <h2>Skills</h2>

            </div>

            <div class="div4 common-template" id="othersTemplate">
                <h2>Others</h2>

            </div>
            <div class="div4 common-template" id="referencesTemplate">
                <h2>References</h2>

            </div>

        </div>



    </div>
    <script src="./next.js"></script>
</body>

</html>